{
  "version": "2.0",
  "app_name": "YVbackend",
  "stages": {
    "dev": {
      "api_gateway_stage": "api",
      "manage_iam_role": true,
      "environment_variables": {
        "DJANGO_SETTINGS_MODULE": "server.settings"
      },
      "lambda_memory_size": 512,
      "lambda_timeout": 30
    }
  },
  "excludes": [
    ".git/*",
    ".venv/*",
    "tests/*",
    "staticfiles/*",
    "media/*",
    "*.sqlite3",
    "*.pyc",
    "__pycache__/*"
  ]
}
